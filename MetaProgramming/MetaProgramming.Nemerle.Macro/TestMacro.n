using System;
using Nemerle;
using Nemerle.Collections;
using Nemerle.Compiler;
using Nemerle.Text;
using Nemerle.Utility;

namespace MetaProgramming.Nemerle.Macro
{
    // Test macros to show difference between compile time and runtime execution
    public macro TestMacro(input)
    {
        Console.WriteLine("Compile-time execution: {0}\n", input);
        <[ Console.WriteLine("Run-time execution: {0}\n", $input) ]>;
    }
}
